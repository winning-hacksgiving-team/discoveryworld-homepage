@media (max-width: 667px) {

  .sb_post-data,.sb_text-size {
    display: none;
  }
  .sb_share li.sbfacebook a {
    width: 34px;
    padding: 0;
  }
  .sb_actions {
    float: right;
    margin-right: 5px;
  }
  #sb_super_bar .sb_share {
    padding: 6px 10px;
  }
  .sb_next_post_image {
    display:none;
  }


}


@media (max-width: 320px) {

  .sb_prev-next-posts > a {
    width: 32px;
  }
  .sb_actions a, .sb_actions a:visited {
    padding:0;
  }

}































